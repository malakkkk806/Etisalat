# Etisalat UPG Payment Gateway Configuration
# Copy this file to .env and fill in your actual credentials

# Merchant Credentials (Replace with your actual values)
MERCHANT_ID=YOUR_MERCHANT_ID_HERE
TERMINAL_ID=YOUR_TERMINAL_ID_HERE
MERCHANT_SECRET_KEY=YOUR_MERCHANT_SECRET_KEY_HERE

# API Configuration
UPG_BASE_URL=https://upg.egyptianbanks.com/cube/paylink.svc/api
UPG_ENVIRONMENT=production

# Server Configuration
PORT=3000
NODE_ENV=production

# Default Payment Settings
DEFAULT_CURRENCY=818
DEFAULT_VALIDITY_MINUTES=30
